<!DOCTYPE html>
<html>
  <head>
    <title>Sorgente</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <div> 
    <%= link_to "Logout", logout_path, method: :delete %>
    <%= link_to "Add Resource", new_resource_path %>
    <%= link_to "All Resources", resources_path %>
    <%= link_to "All Topics", topics_path %>
    <%= link_to "All Reviews", reviews_path %>
    <%= link_to "Write a New Review", new_review_path %>
  </div>
  <body>
    <%= yield %>
  </body>
</html>
